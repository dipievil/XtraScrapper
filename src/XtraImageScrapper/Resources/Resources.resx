  <data name="AppName" xml:space="preserve">
    <value>XtraImageScrapper</value>
  </data>
  <data name="AppDescription" xml:space="preserve">
    <value>Scrapper de imagens - Baixa imagens de URLs ou sites</value>
  </data>
  <data name="Help_Usage" xml:space="preserve">
    <value>Uso: XtraImageScrapper.exe [opções]</value>
  </data>
  <data name="Help_Options" xml:space="preserve">
    <value>Opções:</value>
  </data>
  <data name="Help_Url" xml:space="preserve">
    <value>  --url &lt;url&gt;        URL ou arquivo contendo URLs para baixar</value>
  </data>
  <data name="Help_Output" xml:space="preserve">
    <value>  --output &lt;pasta&gt;   Pasta de destino (padrão: appsettings.json)</value>
  </data>
  <data name="Help_Concurrent" xml:space="preserve">
    <value>  --concurrent &lt;n&gt;  Número de downloads simultâneos (padrão: 5)</value>
  </data>
  <data name="Help_Help" xml:space="preserve">
    <value>  --help, -h, /?     Mostra esta mensagem de ajuda</value>
  </data>
