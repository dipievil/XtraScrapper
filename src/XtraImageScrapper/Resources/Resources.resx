    <value>Exemplos:</value>
  </data>
  <data name="Help_Example1" xml:space="preserve">
    <value>  XtraImageScrapper.exe --url "https://example.com/image.jpg"</value>
  </data>
  <data name="Help_Example2" xml:space="preserve">
    <value>  XtraImageScrapper.exe --url "urls.txt" --output "C:\\Imagens"</value>
  </data>
